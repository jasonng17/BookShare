<h1>Listing Fellow BookSharers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>School</th>
      <th>Department</th>
      <th>Contact</th>
      <th>Email</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @user.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.school %></td>
        <td><%= user.department %></td>
        <td><%= user.contact %></td>
        <td><%= user.email %></td>
        <td><%= link_to 'Show bookshelf',user_books_path(user) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
